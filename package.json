{
  "name": "h5-nuxt-mint",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "杨家辉 <yangjiahui@yy.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt -p 3000 -H 0.0.0.0",
    "dev-spa": "nuxt --spa -p 3000 -H 0.0.0.0",
    "build": "nuxt build",
    "build-a": "nuxt build --analyze",
    "build-spa": "nuxt build --spa",
    "start": "nuxt start -p 3000 -H 0.0.0.0",
    "start-spa": "nuxt start --spa -p 3000 -H 0.0.0.0",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "mint-ui": "^2.2.9",
    "nuxt": "^1.0.0-rc11",
    "vue-cookie": "^1.1.4",
    "vue-i18n": "^5.0.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-component": "^0.10.1",
    "eslint": "^4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6"
  }
}
